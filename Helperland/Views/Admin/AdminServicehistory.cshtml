                </select>
                <select class="form-select" onchange="SelectedTextValue(this)">
                    <option selected>Service Provider</option>
                    <option value="1">Demo due</option>
                    <option value="2">Jhon karthy</option>
                    <option value="2">piggy boy</option>
                </select>
                <select class="form-select" style="width: 120px;" onchange="SelectedTextValue(this)">
                    <option value="Nothing" selected>Status</option>
                    <option value="1">New</option>
                    <option value="2">Completed</option>
                    <option value="4">Pending</option>
                    <option value="3">Cancelled</option>
                </select>
                <input type="text" id="myInput" class="d-none" />
                <div class="input-group me-2">
                    <span class="input-group-text" id="basic-addon1" style="background-color: #fff; border-right: none;"> <img src="~/IMAGES/calendar-blue.png" alt="calander"> </span>
                    <input type="date" class="form-control" id="from-date" style="border-left: none;" placeholder="From Date" aria-label="date" aria-describedby="basic-addon1" >

                </div>
                <div class="input-group me-2">
                    <span class="input-group-text" id="basic-addon1" style="background-color: #fff; border-right: none;"> <img src="~/IMAGES/calendar-blue.png" alt="calander"> </span>
                    <input type="date" class="form-control" id="to-date" style="border-left: transparent;" placeholder="To Date" aria-label="date" aria-describedby="basic-addon1" >

                </div>
                <input class="submit-btn button" type="button" value="search" onclick="myFunction()">
                <input class="reset-btn button" id="reset-btn" type="reset" value="clear">
            </form>
        </div>

        <!--filer code end-->
        <!--table code start-->
