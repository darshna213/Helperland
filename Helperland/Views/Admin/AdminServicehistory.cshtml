</div>
<!--admin-service-history code end-->
<!--edit-reschedule modal code start-->
<input class="edit-hidden-input d-none" data-target="#accept-modal" data-toggle="modal" type="button" id="edit-modal-open-button">
<div class="modal fade" id="accept-modal" tabindex="-1" role="dialog" aria-labelledby="exampleModal2Label" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title " id="exampleModal2Label" style=" font-weight: bold; color: #646464;">Edit Service Request</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form method="post">
                    <div class="form-group ">
                        <div class="add-address-box" id="add-adress-box-modal">

                            <div class="address-taker-box d-flex" style="width:100%;">
                                <div style="width:45%;">
                                    <label for="date">
                                        <b>Date</b>
                                    </label>
                                    <br>
                                    <input type="date" name="date" id="edit-date-modal" />
                                </div>
                                <div style="width:45%;">
                                    <label for="time">
                                        <b>Time</b>
                                    </label>
                                    <br>
                                    <select id="edit-time-modal" style="color:#646464">
                                        <option selected value="2:00">2:00</option>
                                        <option value="4:00">4:00</option>
                                        <option value="6:00">6:00</option>
                                        <option value="8:00">8:00</option>
                                    </select>
                                    <input hidden type="number" id="editId" name="editID" />
                                </div>
                            </div>
                            <div style="color: #646464; margin-left: 20px; padding-left: 10px;">
                                <p><b>Service Address</b></p>
                            </div>
                            <div class="address-taker-box d-flex" style="width:100%;">
                                <div style="width:45%;">
                                    <label for="streetname">
                                        Street name
                                    </label>
                                    <br>
                                    <input type="text" name="streetname" placeholder="Street name" id="edit-streetname-modal" />
                                </div>
                                <div style="width:45%;">
                                    <label for="housename">
                                        House number
                                    </label>
                                    <br>

                                    <input type="text" name="housename" placeholder="House number" id="edit-housename-modal" />
                                </div>
                            </div>
                            <div class="address-taker-box d-flex" style="width:100%;">
                                <div style="width:45%;">
                                    <label for="postalcode">
                                        Postal code
                                    </label>
                                    <br>
                                    <input type="text" name="postalcode" placeholder="Postal code" id="edit-postalcode-modal" />

                                </div>
                                <div style="width:45%;">
                                    <label for="city">
                                        City
                                    </label>
                                    <br>
                                    <input type="text" name="city" placeholder="City" id="edit-city-modal" />
                                </div>
                            </div>
                            <div style="color: #646464; margin-left: 20px; padding-left: 10px;">
                                <p><b>Invoice Address</b></p>
                            </div>
                            <div class="address-taker-box d-flex" style="width:100%;">
                                <div style="width:45%;">
                                    <label for="streetname">
                                        Street name
                                    </label>
                                    <br>
                                    <input type="text" name="streetname" placeholder="Street name" id="invoice-edit-streetname-modal" />
                                </div>
                                <div style="width:45%;">
                                    <label for="housename">
                                        House number
                                    </label>
                                    <br>

                                    <input type="text" name="housename" placeholder="House number" id="invoice-edit-housename-modal" />
                                </div>
                            </div>
                            <div class="address-taker-box d-flex" style="width:100%;">
                                <div style="width:45%;">
                                    <label for="postalcode">
                                        Postal code
                                    </label>
                                    <br>
                                    <input type="text" name="postalcode" placeholder="Postal code" id="invoice-edit-postalcode-modal" />

                                </div>
                                <div style="width:45%;">
                                    <label for="city">
                                        City
                                    </label>
                                    <br>
                                    <input type="text" name="city" placeholder="City" id="invoice-edit-city-modal" />
                                </div>
                            </div>
                            <div class="address-taker-box " style="width:100%;">
                                <label class="accept-heading"><b>Why do you want to reschedule service request?</b></label>
                                <textarea class="modal-textarea" id="reschedule-modal-textarea" style="color: #646464; width: 90%; border: 1px solid #c8c8c8; border-radius: 5px;" placeholder="Why do you want to reschedule service request?"></textarea>
                            </div>
                            <div class="address-taker-box " style="width:100%;">
                                <label class="accept-heading"><b>Call Center EMP Notes</b></label>
                                <textarea class="modal-textarea" id="emp-modal-textarea" style="color: #646464; width: 90%; border: 1px solid #c8c8c8; border-radius: 5px;" placeholder="Enter Notes"></textarea>
                            </div>
                        </div>
                    </div>
                    <button class="update-now rounded-pill m-auto d-flex align-items-center justify-content-center" id="update-now-btn" type="button" data-dismiss="modal">
                        Update
                    </button>
                </form>
            </div>
        </div>
    </div>
</div>
<!--edit-reschedule modal code end-->
<!--cancel modal code start-->
<input class="cancel-hidden-input d-none" data-target="#cancel-modal" data-toggle="modal" type="button" id="cancel-modal-open-button">
<div class="modal fade" id="cancel-modal" tabindex="-1" role="dialog" aria-labelledby="exampleModal2Label" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title cancel-service-request" id="exampleModal2Label">Cancel Service Request</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form method="post">
                    <div class="form-group ">
                        <input type="hidden" id="cancelId" name="cacnelID" />
                        <p class="cancel-heading">Why you want to cancel the service request?</p>
                        <textarea class="modal-textarea" id="cancel-modal-textarea" style="color:#646464;" required></textarea>
                    </div>
                    <button class="cancel-now rounded-pill m-auto d-flex align-items-center justify-content-center" id="cancel-now-btn" type="button" data-dismiss="modal">
                        Cancel Now
                    </button>
                </form>
            </div>
        </div>
    </div>
</div>
<!--cancel modal code end-->